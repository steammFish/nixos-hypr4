{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "images/logo.png",
      "default_popup": "popup.html",
      "default_title": "LingoCloud"
   },
   "commands": {
      "translate-page": {
         "description": "__MSG_launchApp__",
         "suggested_key": {
            "default": "Alt+Shift+P",
            "mac": "Command+Shift+P"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "/css/sites/jsceet.cn.css" ],
      "matches": [ "*://*.jsceet.cn/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "homepage_url": "https://fanyi.caiyunapp.com",
   "icons": {
      "128": "images/logo.png",
      "16": "images/logo.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "optional_permissions": [  ],
   "options_ui": {
      "chrome_style": false,
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "activeTab", "cookies", "storage", "tabs", "webNavigation", "\u003Call_urls>", "*://*.caiyunapp.com/*", "*://*.caiyunai.com/*", "contextMenus", "tabCapture" ],
   "short_name": "__MSG_appNameAbbr__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.14.1",
   "web_accessible_resources": [ "assets/*", "trs.js", "images/*", "css/*", "bootPage.html", "bootPage.js", "options.html", "options.js", "lib/*" ]
}
