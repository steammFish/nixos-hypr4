{
   "background": {
      "scripts": [ "bg.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "img/logo-16.png",
         "48": "img/logo-48.png",
         "64": "img/logo-64.png"
      }
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Alt+P",
            "linux": "Alt+P",
            "mac": "Alt+P",
            "windows": "Alt+P"
         }
      }
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "img/logo-128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_name__",
   "permissions": [ "https://*/*", "http://*/*", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6.2"
}
